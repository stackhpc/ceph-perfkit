[global]
bs=8k
iodepth=16
direct=1
ioengine=libaio
randrepeat=0
group_reporting
time_based
runtime=60
filesize=2G

[sdd]
write_bw_log=hdd-read-3-sdd.log
rw=read
filename=/dev/sdd
name=hdd-read-3
[sde]
write_bw_log=hdd-read-3-sde.log
rw=read
filename=/dev/sde
name=hdd-read-3
[sdj]
write_bw_log=hdd-read-3-sdj.log
rw=read
filename=/dev/sdj
name=hdd-read-3
