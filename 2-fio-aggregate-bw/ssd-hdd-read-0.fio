[global]
bs=8k
iodepth=16
direct=1
ioengine=libaio
randrepeat=0
group_reporting
time_based
runtime=60
filesize=2G

[sdb]
write_bw_log=ssd-hdd-read-0-sdb.log
rw=read
filename=/dev/sdb
name=ssd-hdd-read-0
[sdc]
write_bw_log=ssd-hdd-read-0-sdc.log
rw=read
filename=/dev/sdc
name=ssd-hdd-read-0
[sdi]
write_bw_log=ssd-hdd-read-0-sdi.log
rw=read
filename=/dev/sdi
name=ssd-hdd-read-0
