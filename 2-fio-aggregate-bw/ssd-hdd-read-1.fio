[global]
bs=8k
iodepth=16
direct=1
ioengine=libaio
randrepeat=0
group_reporting
time_based
runtime=60
filesize=2G

[sdb]
write_bw_log=ssd-hdd-read-1-sdb.log
rw=read
filename=/dev/sdb
name=ssd-hdd-read-1
[sdc]
write_bw_log=ssd-hdd-read-1-sdc.log
rw=read
filename=/dev/sdc
name=ssd-hdd-read-1
[sdi]
write_bw_log=ssd-hdd-read-1-sdi.log
rw=read
filename=/dev/sdi
name=ssd-hdd-read-1

[sdd]
write_bw_log=ssd-hdd-read-1-sdd.log
rw=read
filename=/dev/sdd
name=ssd-hdd-read-1
