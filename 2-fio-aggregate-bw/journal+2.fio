[global]
bs=8k
iodepth=16
direct=1
ioengine=libaio
randrepeat=0
group_reporting
time_based
runtime=60
filesize=2G

[journal]
write_bw_log=journal+2-journal.log
rw=read
filename=/dev/sdi
name=journal+2
[sdb]
write_bw_log=journal+2-sdb.log
rw=read
filename=/dev/sdb
name=journal+2
[sdj]
write_bw_log=journal+2-sdj.log
rw=read
filename=/dev/sdj
name=journal+2
