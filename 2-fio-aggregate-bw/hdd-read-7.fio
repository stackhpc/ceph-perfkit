rw=read
filename=/dev/sdg
name=hdd-read-7
[sdj]
write_bw_log=hdd-read-7-sdj.log
rw=read
filename=/dev/sdj
name=hdd-read-7
[sdk]
write_bw_log=hdd-read-7-sdk.log
rw=read
